const React = require('react')
const ReactDOM = require('react-dom')
const { BrowserRouter, Switch } = require('react-router-dom')
const generatorRoute = require('../util/generatorRoute')
<% cssFiles.forEach(function (filePath) { %>
require("<%= filePath %>")
<% }) %>


const renderFunctions = <%- renderElementFuntions %>
function App(){
return (
  <Switch>
    {
      generatorRoute(<%- routes %>, renderFunctions)
    }
  </Switch>
  )
}

ReactDOM.render(
<BrowserRouter>
  <App />
</BrowserRouter>,
document.getElementById('app')
);
